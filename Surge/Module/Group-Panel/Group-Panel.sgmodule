#!name=Group-Panel
#!desc=快速切换节点

[Panel]
Group-Panel = script-name=Group-Panel,update-interval=1

[Script]
Group-Panel = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Panel/Group-Panel/Group-Panel.js,argument=icon=network&color=#86abee&group=Master
  对应参数：
	icon：图标
	color：图标颜色
	group：策略组名称